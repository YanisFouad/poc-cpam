<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
	</head>
	<body>
		{% include "flash.html.twig" %}
		{% include "header.html.twig" %}
		<main id="main">
			{% block body %}{% endblock %}
		</main>
		{% include "footer.html.twig" %}
	</body>
</html>
